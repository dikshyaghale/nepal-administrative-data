    "census",
    "search",
    "utility",
    "geographic",
    "south-asia"
  ],
  "author": "Dikshya Ghale",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dikshyaghale/nepal-administrative-data.git"
